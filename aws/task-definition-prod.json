{
  "executionRoleArn": "arn:aws:iam::657600230790:role/ecsTaskExecutionRole",
  "family": "ehr-app-prod",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "ehr-app-prod",
      "image": "TBD",
      "cpu": 256,
      "memoryReservation": 512,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ehr-app-prod",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "environment": [
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
