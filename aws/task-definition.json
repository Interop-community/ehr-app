{
  "family": "ehr-app-test",
  "containerDefinitions": [
    {
      "name": "ehr-app-test",
      "image": "hspconsortium/ehr-app:0.6.0",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "memoryReservation": 100,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ehr-app-test",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "essential": true,
      "environment": [
        {
          "name": "JASYPT_ENCRYPTOR_PASSWORD",
          "value": ""
        },
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": ""
        },
        {
          "name": "TARGET_ENV",
          "value": "test"
        }
      ]
    }
  ]
}
